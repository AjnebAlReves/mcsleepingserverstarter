{
  "name": "mcsleepingserverstarter",
  "version": "1.16.4",
  "description": "Sleeps until someone connects",
  "main": "sleepingServerStarter.js",
  "scripts": {
    "start": "ts-node sleepingServerStarter.ts",
    "prebuild": "rimraf ./build/ ./bin/",
    "build": "npm-run-all build:*",
    "build:typescript": "tsc",
    "build:win": "pkg build/sleepingServerStarter.js --target win --output bin/sleepingServerStarter.exe",
    "build:linux": "pkg build/sleepingServerStarter.js --target linux --output bin/sleepingServerStarter.run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@jsprismarine/prismarine": "0.1.0-rc.44",
    "@jsprismarine/raknet": "0.1.0-rc.44",
    "connect": "^3.7.0",
    "js-yaml": "^4.0.0",
    "minecraft-protocol": "^1.23.3",
    "serve-static": "^1.14.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/connect": "^3.4.34",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.34",
    "@types/serve-static": "^1.13.9",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "author": "Vincss",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincss/mcsleepingserverstarter.git"
  },
  "keywords": [
    "maintenance",
    "minecraft",
    "nodejs",
    "emptyserverstopper",
    "sleepingserver"
  ],
  "bugs": {
    "url": "https://github.com/vincss/mcsleepingserverstarter/issues"
  },
  "homepage": "https://github.com/vincss/mcsleepingserverstarter#readme",
  "engines": {
    "node": ">=14"
  }
}
