{
  "name": "mcsleepingserverstarter",
  "version": "1.16.4",
  "description": "Sleeps until someone connects",
  "main": "sleepingServerStarter.js",
  "scripts": {
    "start": "ts-node sleepingServerStarter.ts",
    "build": "npm-run-all build:*",
    "build:win": "pkg sleepingServerStarter.ts --target win --output bin/sleepingServerStarter.exe",
    "build:linux": "pkg sleepingServerStarter.ts --target linux --output bin/sleepingServerStarter.run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "connect": "^3.7.0",
    "js-yaml": "^3.13.1",
    "minecraft-protocol": "^1.23.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "pkg": "^4.4.9",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "npm-run-all": "^4.1.5",
    "serve-static": "^1.14.1"
  },
  "author": "Vincss",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vincss/mcsleepingserverstarter.git"
  },
  "keywords": [
    "maintenance",
    "minecraft",
    "nodejs",
    "emptyserverstopper",
    "sleepingserver"
  ],
  "bugs": {
    "url": "https://github.com/vincss/mcsleepingserverstarter/issues"
  },
  "homepage": "https://github.com/vincss/mcsleepingserverstarter#readme",
  "engines": {
    "node": ">=14"
  }
